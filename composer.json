{
    "name": "symfony/framework-standard-edition",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.3.*@stable",
        "doctrine/orm": ">=2.2.3,<2.4-dev",
        "doctrine/doctrine-bundle": "1.3.*",
        "twig/extensions": "1.2.0",
        "symfony/assetic-bundle": "2.3.*@stable",
        "symfony/swiftmailer-bundle": "2.3.*",
        "symfony/monolog-bundle": "2.3.*",
        "sensio/distribution-bundle": "2.3.*",
        "sensio/framework-extra-bundle": "2.3.*",
        "sensio/generator-bundle": "2.3.*",
        "incenteev/composer-parameter-handler": "~2.0",
        "kriswallsmith/assetic": "1.1.x@stable",
        
        "doctrine/doctrine-fixtures-bundle": "dev-master",
        "gedmo/doctrine-extensions": " 2.3.*",
        "stof/doctrine-extensions-bundle": "dev-master",

        "friendsofsymfony/user-bundle": "~1.3",

        "whiteoctober/breadcrumbs-bundle": "dev-master",

        "stfalcon/tinymce-bundle": "dev-master",

        "knplabs/gaufrette": ">=0.1.6",
        "imagine/Imagine": "*@stable",
        "kriswallsmith/buzz": "0.*",

        "sonata-project/core-bundle": "^2.3",
        "sonata-project/notification-bundle": "^2.3",
        "sonata-project/easy-extends-bundle": "^2.1",
        "sonata-project/media-bundle": "dev-master",
        "sonata-project/admin-bundle": "dev-master",
        "sonata-project/exporter": "^1.3",
        "sonata-project/block-bundle": "^2.3",
        "sonata-project/cache-bundle": "^2.2",
        "sonata-project/doctrine-orm-admin-bundle": "dev-master",
        "sonata-project/intl-bundle" : "^2.2",
        "sonata-project/doctrine-extensions": "^1.0",
        "sonata-project/google-authenticator": "^1.0",
        "sonata-project/user-bundle": "dev-master",
        "sonata-project/classification-bundle": "dev-master",
        "sonata-project/datagrid-bundle": "dev-master",

        "jms/serializer-bundle": "~0.11",

        "simplethings/entity-audit-bundle": "0.7",

        "white-october/pagerfanta-bundle": "dev-master",
        "pagerfanta/pagerfanta": "dev-master",
        "herzult/forum-bundle": "dev-master",

        "willdurand/geocoder": "1.1.*@dev",
        "egeloen/google-map-bundle": "dev-master",

        "coresphere/console-bundle": "dev-master",

        "eko/feedbundle": "dev-master",
        "ezyang/htmlpurifier": "4.5.*",
        "desarrolla2/cache": "1.*",
        "desarrolla2/rss-client": "~2",
        "desarrolla2/rss-client-bundle": "dev-master",
        "sonata-project/formatter-bundle": "^2.3",
        "phpunit/phpunit": "~4.4",

        "gregwar/captcha-bundle": "dev-master",
        "sonata-project/seo-bundle": "^2.0"
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web"
    }
}
