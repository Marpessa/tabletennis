<div class="col-md-6">
    <div class="headline"><span class="title">En progression sur {{ lastMonthPoint }}</span></div>

    {% for evo in evo_best_list %}
        <h3 class="heading-xs no-top-space">
            <a class='name' href='{{ path('_sonataUserDashboard', { "licensee_number" : evo.licensee_number ,"slug": evo.slug } ) }}'>
                {{ evo.name | truncate(85) | striptags | raw }}
            </a>
            <span class="pull-right">+{{ evo.points }}</span>
        </h3>
        <div class="progress progress-u progress-xs">
            <div style="width: {{ evo.points }}%" aria-valuemax="100" aria-valuemin="0" aria-valuenow="{{ evo.points }}" role="progressbar" class="progress-bar progress-bar-u"></div>
        </div>
    {% endfor %}
</div><!--/col-md-6-->

<div class="col-md-6">
    <div class="headline"><span class="title">En régression sur {{ lastMonthPoint }}</span></div>

    {% for evo in evo_worst_list %}
        <h3 class="heading-xs no-top-space">
            <a class='name' href='{{ path('_sonataUserDashboard', { "licensee_number" : evo.licensee_number ,"slug": evo.slug } ) }}'>
                {{ evo.name | truncate(85) | striptags | raw }}
            </a>
            <span class="pull-right">{{ evo.points }}</span>
        </h3>
        <div class="progress progress-u progress-xs">
            <div style="width: {{ evo.points|abs }}%" aria-valuemax="100" aria-valuemin="0" aria-valuenow="{{ evo.points|abs }}" role="progressbar" class="progress-bar progress-bar-red"></div>
        </div>
    {% endfor %}
</div><!--/col-md-6-->

{#}
<section id='best_evolution' class='evolution fleft'>
    <h4>Les progressions de {{ lastMonthPoint }}</h4>
    <ul>
        {% for evo in evo_best_list %}
        <li>
            <a class='name' href='{{ path('_sonataUserDashboard', { "licensee_number" : evo.licensee_number ,"slug": evo.slug } ) }}'>
                {{ evo.name | truncate(85) | striptags | raw }}
                <span class='points fright'>+{{ evo.points }}</span>
            </a>
        </li>
        {% endfor %}
    </ul>
</section>

<section id='worst_evolution' class='evolution fleft'>
    <h4>Les régressions de {{ lastMonthPoint }}</h4>
    <ul>
         {% for evo in evo_worst_list %}
        <li>
            <a class='name' href='{{ path('_sonataUserDashboard', { "licensee_number" : evo.licensee_number ,"slug": evo.slug } ) }}'>
                {{ evo.name | truncate(85) | striptags | raw }}
                <span class='points fright'>{{ evo.points }}</span>
            </a>
        </li>
        {% endfor %}
    </ul>
</section>
{#}