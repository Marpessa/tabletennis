        <style>
            html.loading { background-color: #fff !important; }
            html.loading body { background-color: #fff !important; height: 100%; min-height: 600px; }
            html.loading body > * { display: none; }
            html.loading body .loader.show { display: block !important; }

            .loader:before,
            .loader:after,
            .loader {
                display: block;
                border-radius: 50%;
                width: 2.5em;
                height: 2.5em;
                -webkit-animation-fill-mode: both;
                animation-fill-mode: both;
                -webkit-animation: load7 1.8s infinite ease-in-out;
                animation: load7 1.8s infinite ease-in-out;
            }
            .loader {
                display: none;
                margin: 8em auto;
                font-size: 10px;
                position: absolute;
                left: 0;
                right: 0;
                top: 45%;
                bottom: 0;
                margin-top: -25px;

                text-indent: -9999em;
                -webkit-transform: translateZ(0);
                -ms-transform: translateZ(0);
                transform: translateZ(0);
                -webkit-animation-delay: -0.16s;
                animation-delay: -0.16s;
            }
            .loader:before {
                left: -3.5em;
                -webkit-animation-delay: -0.32s;
                animation-delay: -0.32s;
            }
            .loader:after {
                left: 3.5em;
            }
            .loader:before,
            .loader:after {
                content: '';
                position: absolute;
                top: 0;
            }
            @-webkit-keyframes load7 {
              0%,
              80%,
              100% {
                box-shadow: 0 2.5em 0 -1.3em #3498db;
              }
              40% {
                box-shadow: 0 2.5em 0 0 #3498db;
              }
            }
            @keyframes load7 {
              0%,
              80%,
              100% {
                box-shadow: 0 2.5em 0 -1.3em #3498db;
              }
              40% {
                box-shadow: 0 2.5em 0 0 #3498db;
              }
            }
        </style>

