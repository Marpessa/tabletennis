        {% block stylesheets %}
            <script>
                (function() {
                    var sourcesCss = [];
                    
                    {% stylesheets 'assets/plugins/bootstrap/css/bootstrap.min.css'
                                   'assets/css/style.css'
                                   'assets/css/plugins.css'
                                   'assets/css/plugins/box-shadows.css'
